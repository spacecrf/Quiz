    <choices>
      <choice>Percussion</choice>
      <choice>String</choice>
      <choice correct="yes">Wind</choice>
      <choice>Brass</choice>
    </choices>
  </question>
  <question id="19">
    <wording>Which wind instrument usually uses a double reed?</wording>
    <choices>
      <choice>Clarinet</choice>
      <choice>Saxophone</choice>
      <choice correct="yes">Oboe</choice>
      <choice>Flute</choice>
    </choices>
  </question>
  <question id="20">
    <wording>Which keyboard instrument is commonly used in electronic music?</wording>
    <choices>
      <choice>Grand piano</choice>
      <choice>Clavichord</choice>
      <choice correct="yes">Synthesizer</choice>
      <choice>Harpsichord</choice>
    </choices>
  </question>
</test>
