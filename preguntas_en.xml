<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE test [
  <!ELEMENT test (question+)>
  <!ELEMENT question (wording,choices)>
  <!ELEMENT wording (#PCDATA)>
  <!ELEMENT choices (choice+)>
  <!ELEMENT choice (#PCDATA)>
  <!ATTLIST question id CDATA #REQUIRED>
  <!ATTLIST choice correct (yes|no) 'no'>
]>
<test>
  <question id="1">
    <wording>Which of these instruments belongs to the string family?</wording>
    <choices>
      <choice correct="yes">Violin</choice>
      <choice>Piano</choice>
      <choice>Flute</choice>
      <choice>Trombone</choice>
    </choices>
  </question>
  <question id="2">
    <wording>Which instrument has keys and strings inside?</wording>
    <choices>
      <choice>Tambourine</choice>
      <choice>Guitar</choice>
      <choice correct="yes">Piano</choice>
      <choice>Oboe</choice>
    </choices>
  </question>
  <question id="3">
    <wording>Which string instrument is played with a bow?</wording>
    <choices>
      <choice>Guitar</choice>
      <choice>Harp</choice>
      <choice correct="yes">Cello</choice>
      <choice>Ukulele</choice>
    </choices>
  </question>
  <question id="4">
    <wording>Which is a percussion instrument?</wording>
    <choices>
      <choice>Flute</choice>
      <choice correct="yes">Drums</choice>
      <choice>Viola</choice>
      <choice>Clarinet</choice>
    </choices>
  </question>
  <question id="5">
    <wording>Which instrument is a woodwind?</wording>
    <choices>
      <choice>Trumpet</choice>
      <choice>Horn</choice>
      <choice correct="yes">Flute</choice>
      <choice>Tuba</choice>
    </choices>
  </question>
  <question id="6">
    <wording>Which is the lowest-sounding string instrument?</wording>
    <choices>
      <choice>Viola</choice>
      <choice>Violin</choice>
      <choice>Cello</choice>
      <choice correct="yes">Double bass</choice>
    </choices>
  </question>
  <question id="7">
    <wording>Which instrument belongs to the brass family?</wording>
    <choices>
      <choice>Clarinet</choice>
      <choice>Bassoon</choice>
      <choice correct="yes">Trombone</choice>
      <choice>Oboe</choice>
    </choices>
  </question>
  <question id="8">
    <wording>What part is pressed on a guitar to change notes?</wording>
    <choices>
      <choice>Body</choice>
      <choice>Rosette</choice>
      <choice correct="yes">Frets</choice>
      <choice>Bridge</choice>
    </choices>
  </question>
  <question id="9">
    <wording>Which instrument is triangle-shaped and struck to play?</wording>
    <choices>
      <choice>Tambourine</choice>
      <choice>Maracas</choice>
      <choice correct="yes">Triangle</choice>
      <choice>Cowbell</choice>
    </choices>
  </question>
  <question id="10">
    <wording>Which family of instruments produces sound by vibrating a column of air?</wording>
    <choices>
      <choice correct="yes">Wind</choice>
      <choice>Percussion</choice>
      <choice>String</choice>
      <choice>Electronic</choice>
    </choices>
  </question>
  <question id="11">
    <wording>Which instrument has pedals and many vertical strings?</wording>
    <choices>
      <choice>Lute</choice>
      <choice>Electric guitar</choice>
      <choice correct="yes">Harp</choice>
      <choice>Mandolin</choice>
    </choices>
  </question>
  <question id="12">
    <wording>Which wind instrument has a mouthpiece and spiral shape?</wording>
    <choices>
      <choice>Clarinet</choice>
      <choice>Flute</choice>
      <choice correct="yes">French horn</choice>
      <choice>Oboe</choice>
    </choices>
  </question>
  <question id="13">
    <wording>Which instrument is often tuned with an electronic tuner and played with fingers?</wording>
    <choices>
      <choice>Flute</choice>
      <choice correct="yes">Guitar</choice>
      <choice>Violin</choice>
      <choice>Marimba</choice>
    </choices>
  </question>
  <question id="14">
