    <choices>
      <choice>Drum</choice>
      <choice correct="yes">Xylophone</choice>
      <choice>Tambourine</choice>
      <choice>Bass drum</choice>
    </choices>
  </question>
  <question id="15">
    <wording>Which instrument is not part of the classical orchestra?</wording>
    <choices>
      <choice>Oboe</choice>
      <choice>Flute</choice>
      <choice correct="yes">Electric guitar</choice>
      <choice>Timpani</choice>
    </choices>
  </question>
  <question id="16">
    <wording>Which is a bowed string instrument?</wording>
    <choices>
      <choice>Piano</choice>
      <choice correct="yes">Violin</choice>
      <choice>Banjo</choice>
      <choice>Guitar</choice>
